{
  "manifest_version": 3,
  "name": "PomoDummy",
  "description": "PomoDummy",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "PomoDummy"
  },
  "permissions": ["scripting", "storage", "tabs", "windows"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"],
      "css": ["scripts/content.css"]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js"
  }
}
